{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/server",
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["server/**/*.ts"]
}
